   "outputs": [],
   "source": [
    "# open the file\n",
    "with sr.AudioFile('wavfile.wav') as source:\n",
    "    # listen for the data (load audio to memory)\n",
    "    audio_data = r.record(source)\n",
    "    # recognize (convert from speech to text)\n",
    "    text = r.recognize_google(audio_data)\n",
    "    print(text)"
